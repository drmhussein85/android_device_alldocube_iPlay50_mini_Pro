cc_defaults {
    name: "libgptutils.iPlay50_mini_Pro",
    export_include_dirs: ["."],
    //header_libs: ["generated_kernel_headers"],
    recovery_available: true,
    shared_libs: [
        "libcutils",
        "liblog",
        "libz",
    ],
    srcs: [
        "gpt-utils.cpp",
        "recovery-ufs-bsg.cpp",
    ],
    vendor: true,
}

cc_library {
    name: "libgptutils-sg.iPlay50_mini_Pro",
    defaults: ["libgptutils.iPlay50_mini_Pro"],
}

cc_library {
    name: "libgptutils-bsg.iPlay50_mini_Pro",
    defaults: ["libgptutils.iPlay50_mini_Pro"],
    cflags: ["-D_BSG_FRAMEWORK_KERNEL_HEADERS"],
}
